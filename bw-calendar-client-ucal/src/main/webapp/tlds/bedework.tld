    <name>emitCurrentPrivs</name>
    <tagclass>org.bedework.webcommon.taglib.EmitCurrentPrivsTag</tagclass>
    <bodycontent>empty</bodycontent>
    <info>Emit xml version of current privs in a tag</info>

