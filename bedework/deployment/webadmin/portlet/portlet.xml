<?xml version="1.0" encoding="UTF-8" ?>
<!--
  Based on jpetstore example application with contributions from:

  Author: Xavier Lawrence @ jahia . com
  Author: Mike Douglass    douglm@rpi.edu
-->
<portlet-app id="calAdmin" version="1.0">
  <portlet id="@CONTEXT-ROOT@.@PORTLET-NAME@">
    <portlet-name>@PORTLET-NAME@</portlet-name>
    <display-name>@DISPLAY-NAME@</display-name>
    <portlet-class>@PORTLET-CLASS@</portlet-class>
    <init-param>
      <name>ServletContextProvider</name>
      <value>@PORTAL-SERVLET-CONTEXT-PROVIDER@</value>
    </init-param>
    <init-param>
      <name>ViewPage</name>
      <value>/initialise.rdo</value>
    </init-param>
    <description>@APP-DESCRIPTION@
    </description>
    <expiration-cache>-1</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
      <!--<portlet-mode>HELP</portlet-mode>-->
    </supports>
    <portlet-info>
      <title>@DISPLAY-NAME@</title>
      <short-title>@DISPLAY-NAME@</short-title>
      <keywords>Struts, Bedework, Calendar, Public, Events</keywords>
    </portlet-info>
  </portlet>
</portlet-app>

